public class Main {
    public static void main(String[] args) {

        coche micoche = new coche();


        micoche.SumarPuertas();

        System.out.println(micoche.puertas);
    }


    static class coche {
        public int puertas = 2;

        public void SumarPuertas() {
            this.puertas++;
        }
    }
}
